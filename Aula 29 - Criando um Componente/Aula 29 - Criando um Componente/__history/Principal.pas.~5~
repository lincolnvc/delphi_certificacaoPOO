unit Principal;

interface

uses
  System.Classes;

procedure register;

type
  TEventos = class(TComponent)
    private

    public
      function TamanhoString(Value : String) : Integer;
    published
  end;

implementation

{ TEventos }

procedure register;
begin
  RegisterComponents('DelphiUpdates', [TEventos]);
end;


function TEventos.TamanhoString(Value: String): Integer;
begin
  Result := Length(Value);
end;

end.
